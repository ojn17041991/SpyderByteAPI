{
  "ConnectionStrings": {
    "SbApi": "Data Source=.\\Databases\\sbapi.db;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "Imgur": {
    "Url": "https://api.imgur.com/",
    "Version": "3"
  },
  "Authentication": {
    "Issuer": "SpyderByteStudios",
    "Audience": "SpyderByteWebApplication",
    "TimeoutMinutes": 60,
    "Administrator": {
      "UserName": "sbapi_admin"
    },
    "Restricted": {
      "UserName": "sbapi_restricted"
    },
    "Utility": {
      "UserName": "sbapi_utility"
    }
  },
  "Storage": {
    "Containers": {
      "Database": "database"
    }
  },
  "Database": {
    "BackupFileExtension": ".backup"
  },
  "AllowDataClears": false,
  "AllowAuthenticationRefresh": false,
  "AllowedHosts": "*"
}