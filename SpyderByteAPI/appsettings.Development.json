{
  "AllowAuthenticationRefresh": true,
  "AllowedHosts": "*",
  "Authentication": {
    "Issuer": "SpyderByteStudios",
    "Audience": "SpyderByteWebApplication",
    "TimeoutMinutes": 60
  },
  "ConnectionStrings": {
    "SbApi": "Data Source=.\\Databases\\sbapi.db"
  },
  "Database": {
    "BackupFileExtension": ".backup"
  },
  "Encryption": {
    "Argon2": {
      "DegreesOfParallelism": 4,
      "MemorySize": 2048,
      "Iterations": 10
    }
  },
  "FeatureFlags": {
    "AllowDatabaseBackups": true,
    "AllowWriteOperationsOnNonRestrictedUsers": true
  },
  "Imgur": {
    "Url": "https://api.imgur.com/",
    "Version": "3"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "Storage": {
    "Containers": {
      "Database": "database"
    },
    "ClientName": "spyderbyte"
  },
  "Version": {
    "Major": 1,
    "Minor": 0,
    "Patch": 0
  }
}